<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">
<xacro:macro name="igus_rebel_6dof" params="prefix xyz rpy">
  <!-- 加载 UR5 vistion mesh -->
<xacro:property name="mesh_root" value="/home/unvrx/URCell/types/igus_rebel_description_ros2" />

  <link name="${prefix}base_link">
    <visual>
      <geometry>
        <mesh filename="file://${mesh_root}/meshes/igus_rebel_6dof_00/Joint0.dae"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="file://${mesh_root}/meshes/igus_rebel_6dof_00/Joint0Coll.dae"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1.0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>
  <joint name="${prefix}joint1" type="revolute">
    <parent link="base_link"/>
    <child link="link_1"/>
    <origin rpy="0.0 0.0 0.0" xyz="0.0 0 0.100"/>
    <axis xyz="0 0 -1"/>
    <limit effort="330.0" lower="-3.12413936106985" upper="3.12413936106985" velocity="0.7853981633974483"/>
  </joint>
  <link name="${prefix}link_1">
    <visual>
      <geometry>
        <mesh filename="file://${mesh_root}/meshes/igus_rebel_6dof_00/Joint1.dae"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="file://${mesh_root}/meshes/igus_rebel_6dof_00/Joint1Coll.dae"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.03" ixy="0.0" ixz="0.0" iyy="0.03" iyz="0.0" izz="0.03"/>
    </inertial>
  </link>
  <joint name="${prefix}joint2" type="revolute">
    <parent link="link_1"/>
    <child link="link_2"/>
    <origin rpy="0.0 0.5235987755982988 0.0" xyz="0.0 0 0.149"/>
    <axis xyz="0 1 0"/>
    <!-- pi*2/3 are the physical limits, but to avoid any collisions we set the actual limits lower -->
    <limit effort="330.0" lower="-1.9198621771937623" upper="1.9198621771937623" velocity="0.7853981633974483"/>
  </joint>
  <link name="${prefix}link_2">
    <visual>
      <geometry>
        <mesh filename="file://${mesh_root}/meshes/igus_rebel_6dof_00/Joint2.dae"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="file://${mesh_root}/meshes/igus_rebel_6dof_00/Joint2Coll.dae"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.03" ixy="0.0" ixz="0.0" iyy="0.03" iyz="0.0" izz="0.03"/>
    </inertial>
  </link>
  <joint name="${prefix}joint3" type="revolute">
    <parent link="link_2"/>
    <child link="link_5"/>
    <origin rpy="0.0 0.5235987755982988 0.0" xyz="0.0 0.0 0.2384"/>
    <axis xyz="0 1 0"/>
    <!-- pi*2/3 are the physical limits, but to avoid any collisions we set the actual limits lower -->
    <limit effort="330.0" lower="-1.9198621771937623" upper="1.9198621771937623" velocity="0.7853981633974483"/>
  </joint>
  <link name="${prefix}link_5">
    <visual>
      <geometry>
        <mesh filename="file://${mesh_root}/meshes/igus_rebel_6dof_00/Joint3.dae"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="file://${mesh_root}/meshes/igus_rebel_6dof_00/Joint3Coll.dae"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.03" ixy="0.0" ixz="0.0" iyy="0.03" iyz="0.0" izz="0.03"/>
    </inertial>
  </link>
  <joint name="${prefix}joint4" type="revolute">
    <parent link="link_5"/>
    <child link="link_6"/>
    <!-- TODO The 0.03m offset are most likely caused by the mesh file (Issue 15)-->
    <origin rpy="0.0 0.0 0.0" xyz="0.0 0 0.119"/>
    <axis xyz="0 0 1"/>
    <limit effort="330.0" lower="-3.12413936106985" upper="3.12413936106985" velocity="0.7853981633974483"/>
  </joint>
  <link name="${prefix}link_6">
    <visual>
      <geometry>
        <mesh filename="file://${mesh_root}/meshes/igus_rebel_6dof_00/Joint4.dae"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="file://${mesh_root}/meshes/igus_rebel_6dof_00/Joint4Coll.dae"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.03" ixy="0.0" ixz="0.0" iyy="0.03" iyz="0.0" izz="0.03"/>
    </inertial>
  </link>
  <joint name="${prefix}joint5" type="revolute">
    <parent link="link_6"/>
    <child link="link_7"/>
    <!-- TODO The 0.03m offset are most likely caused by the mesh file (Issue 15) -->
    <origin rpy="0.0 -0.1308996938995747 0.0" xyz="0.0 0 0.17"/>
    <axis xyz="0 1 0"/>
    <limit effort="330.0" lower="-1.5271630954950381" upper="1.7889624832941877" velocity="0.7853981633974483"/>
  </joint>
  <link name="${prefix}link_7">
    <visual>
      <geometry>
        <mesh filename="file://${mesh_root}/meshes/igus_rebel_6dof_00/Joint5.dae"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="file://${mesh_root}/meshes/igus_rebel_6dof_00/Joint5Coll.dae"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.03" ixy="0.0" ixz="0.0" iyy="0.03" iyz="0.0" izz="0.03"/>
    </inertial>
  </link>
  <joint name="${prefix}joint6" type="revolute">
    <parent link="link_7"/>
    <child link="link_8"/>
    <origin rpy="0.0 0.0 0.0" xyz="0.0 0 0.1208"/>
    <axis xyz="0 0 1"/>
    <limit effort="330.0" lower="-3.12413936106985" upper="3.12413936106985" velocity="0.7853981633974483"/>
  </joint>
  <link name="${prefix}link_8">
    <visual>
      <geometry>
        <mesh filename="file://${mesh_root}/meshes/igus_rebel_6dof_00/Joint6.dae"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0.0 0 0"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="file://${mesh_root}/meshes/igus_rebel_6dof_00/Joint6Coll.dae"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0.0 0 0"/>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.03" ixy="0.0" ixz="0.0" iyy="0.03" iyz="0.0" izz="0.03"/>
    </inertial>
  </link>
  <joint name="${prefix}joint7" type="fixed">
    <parent link="link_8"/>
    <child link="flange"/>
    <origin rpy="0.0 -1.5707963267948966 0.0" xyz="0.0 0 0.0012"/>
    <axis xyz="0 0 0"/>
  </joint>
  <link name="${prefix}flange">
    </link>
  <joint name="${prefix}flange_to_tool0" type="fixed">
    <parent link="flange"/>
    <child link="tool0"/>
    <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
    <axis xyz="0 0 0"/>
  </joint>
  <link name="${prefix}tool0">
    </link>
  <ros2_control name="igus_rebel_6dof" type="system">
    <hardware>
      <plugin>irc_hardware/IrcRosCan</plugin>
      <param name="can_socket">can0</param>
    </hardware>
    <joint name="${prefix}joint1">
      <command_interface name="position">
        <param name="min">-3.12413936106985</param>
        <param name="max">3.12413936106985</param>
      </command_interface>
     
  </ros2_control>
  <!-- Add world link -->

  <joint name="${prefix}world_to_base" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
    <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
  </joint>
  <!-- Make sure tcp exists even if no tool is attached -->
  <joint name="${prefix}flange_to_tcp" type="fixed">
    <parent link="flange"/>
    <child link="tcp"/>
    <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
    <axis xyz="0 0 0"/>
  </joint>
  <link name="${prefix}tcp">
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.03" ixy="0.0" ixz="0.0" iyy="0.03" iyz="0.0" izz="0.03"/>
    </inertial>
  </link>
</robot>
