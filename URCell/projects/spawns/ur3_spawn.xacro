<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

<xacro:macro name="ur3_spawn_hand" params="prefix xyz rpy">
  <xacro:property name="mesh_root" value="/home/unvrx/URCell/types/" />
  <xacro:property name="joint_limits" value="${xacro.load_yaml('spawns/ur3_joint_limits.yaml')}" />
  <!-- LINKS -->
  <link name="${prefix}base_link" />

  <link name="${prefix}base_link_inertia">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 3.141592653589793"/>
      <geometry><mesh filename="file://${mesh_root}ur_description/meshes/ur3/visual/base.dae"/></geometry>
      <material name="LightGrey"><color rgba="0.7 0.7 0.7 1.0"/></material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 3.141592653589793"/>
      <geometry><mesh filename="file://${mesh_root}ur_description/meshes/ur3/collision/base.stl"/></geometry>
    </collision>
    <inertial>
      <mass value="2.0"/>
      <inertia ixx="0.0030531654454" ixy="0.0" ixz="0.0" iyy="0.0030531654454" iyz="0.0" izz="0.005625"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </inertial>
  </link>

  <link name="${prefix}shoulder_link">
    <inertial>
      <mass value="2.0"/>
      <inertia ixx="0.008093166666666665" ixy="0.0" ixz="0.0" iyy="0.008093166666666665" iyz="0.0" izz="0.005625"/>
      <origin xyz="0.0 0.0 -0.02" rpy="0 0 0"/>
    </inertial>
    <visual>
<origin xyz="0 0 0" rpy="0 0 3.141592653589793"/>
      <geometry><mesh filename="file://${mesh_root}ur_description/meshes/ur3/visual/shoulder.dae"/></geometry>
      <material name="LightGrey"><color rgba="0.7 0.7 0.7 1.0"/></material>
    </visual>
      <material name="LightGrey"><color rgba="0.7 0.7 0.7 1.0"/></material>
    </visual>
    <collision>
<origin xyz="0 0 0" rpy="0 0 3.141592653589793"/>
      <geometry><mesh filename="file://${mesh_root}ur_description/meshes/ur3/collision/shoulder.stl"/></geometry>
    </collision>
    </collision>
  </link>

  <link name="${prefix}upper_arm_link">
    <inertial>
      <mass value="3.42"/>
      <inertia ixx="0.021728491912499998" ixy="0.0" ixz="0.0" iyy="0.021728491912499998" iyz="0.0" izz="0.00961875"/>
      <origin xyz="-0.11365 0.0 0.1157" rpy="0 0 0"/>
    </inertial>
    <visual>
<origin xyz="0 0 0.1198" rpy="1.5707963267948966 0 -1.5707963267948966"/>
      <geometry><mesh filename="file://${mesh_root}ur_description/meshes/ur3/visual/upperarm.dae"/></geometry>
      <material name="LightGrey"><color rgba="0.7 0.7 0.7 1.0"/></material>
    </visual>
      <material name="LightGrey"><color rgba="0.7 0.7 0.7 1.0"/></material>
    </visual>
    <collision>
<origin xyz="0 0 0.1198" rpy="1.5707963267948966 0 -1.5707963267948966"/>
      <geometry><mesh filename="file://${mesh_root}ur_description/meshes/ur3/collision/upperarm.stl"/></geometry>
    </collision>
    </collision>
  </link>

  <link name="${prefix}forearm_link">
    <inertial>
      <mass value="1.26"/>
      <inertia ixx="0.0065468090625" ixy="0.0" ixz="0.0" iyy="0.0065468090625" iyz="0.0" izz="0.00354375"/>
      <origin xyz="-0.16325 0.0 0.0238" rpy="0 0 0"/>
    </inertial>
    <visual>
<origin xyz="0 0 0.0275" rpy="1.5707963267948966 0 -1.5707963267948966"/>
      <geometry><mesh filename="file://${mesh_root}ur_description/meshes/ur3/visual/forearm.dae"/></geometry>
      <material name="LightGrey"><color rgba="0.7 0.7 0.7 1.0"/></material>
    </visual>
      <material name="LightGrey"><color rgba="0.7 0.7 0.7 1.0"/></material>
    </visual>
    <collision>
<origin xyz="0 0 0.0275" rpy="1.5707963267948966 0 -1.5707963267948966"/>
      <geometry><mesh filename="file://${mesh_root}ur_description/meshes/ur3/collision/forearm.stl"/></geometry>
    </collision>
    </collision>
  </link>

  <link name="${prefix}wrist_1_link">
    <inertial>
      <mass value="0.8"/>
      <inertia ixx="0.0016106414999999998" ixy="0.0" ixz="0.0" iyy="0.0016106414999999998" iyz="0.0" izz="0.00225"/>
      <origin xyz="0.0 -0.01 0.0" rpy="0 0 0"/>
    </inertial>
    <visual>
<origin xyz="0 0 -0.085" rpy="1.5707963267948966 0 0"/>
      <geometry><mesh filename="file://${mesh_root}ur_description/meshes/ur3/visual/wrist1.dae"/></geometry>
      <material name="LightGrey"><color rgba="0.7 0.7 0.7 1.0"/></material>
    </visual>
      <material name="LightGrey"><color rgba="0.7 0.7 0.7 1.0"/></material>
    </visual>
    <collision>
<origin xyz="0 0 -0.085" rpy="1.5707963267948966 0 0"/>
      <geometry><mesh filename="file://${mesh_root}ur_description/meshes/ur3/collision/wrist1.stl"/></geometry>
    </collision>
    </collision>
  </link>

  <link name="${prefix}wrist_2_link">
    <inertial>
      <mass value="0.8"/>
      <inertia ixx="0.0015721739999999998" ixy="0.0" ixz="0.0" iyy="0.0015721739999999998" iyz="0.0" izz="0.00225"/>
      <origin xyz="0.0 0.01 0.0" rpy="0 0 0"/>
    </inertial>
    <visual>
<origin xyz="0 0 -0.083" rpy="0 0 0"/>
      <geometry><mesh filename="file://${mesh_root}ur_description/meshes/ur3/visual/wrist2.dae"/></geometry>
      <material name="LightGrey"><color rgba="0.7 0.7 0.7 1.0"/></material>
    </visual>
      <material name="LightGrey"><color rgba="0.7 0.7 0.7 1.0"/></material>
    </visual>
    <collision>
<origin xyz="0 0 -0.083" rpy="0 0 0"/>
      <geometry><mesh filename="file://${mesh_root}ur_description/meshes/ur3/collision/wrist2.stl"/></geometry>
    </collision>
    </collision>
  </link>

  <link name="${prefix}wrist_3_link">
    <inertial>
      <mass value="0.35"/>
      <inertia ixx="0.00013626666666666665" ixy="0.0" ixz="0.0" iyy="0.00013626666666666665" iyz="0.0" izz="0.0001792"/>
      <origin xyz="0.0 0.0 -0.02" rpy="0 0 0"/>
    </inertial>
    <visual>
<origin xyz="0 -0.00255 -0.082" rpy="1.5707963267948966 0 0"/>
      <geometry><mesh filename="file://${mesh_root}ur_description/meshes/ur3/visual/wrist3.dae"/></geometry>
      <material name="LightGrey"><color rgba="0.7 0.7 0.7 1.0"/></material>
    </visual>
      <material name="LightGrey"><color rgba="0.7 0.7 0.7 1.0"/></material>
    </visual>
    <collision>
<origin xyz="0 -0.00255 -0.082" rpy="1.5707963267948966 0 0"/>
      <geometry><mesh filename="file://${mesh_root}ur_description/meshes/ur3/collision/wrist3.stl"/></geometry>
    </collision>
    </collision>
  </link>

  <!-- JOINTS -->
  <joint name="${prefix}base_link-base_link_inertia" type="fixed">
    <parent link="${prefix}base_link"/>
    <child link="${prefix}base_link_inertia"/>
    <origin xyz="0 0 0" rpy="0 0 3.141592653589793"/>
  </joint>

  <joint name="${prefix}shoulder_pan_joint" type="revolute">
    <parent link="${prefix}base_link_inertia"/>
    <child link="${prefix}shoulder_link"/>
    <origin xyz="0 0 0.1519" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="150.0"
      lower="${joint_limits.joint_limits.shoulder_pan_joint.min_position}"
      upper="${joint_limits.joint_limits.shoulder_pan_joint.max_position}"
      velocity="${joint_limits.joint_limits.shoulder_pan_joint.max_velocity}" />
  </joint>

  <joint name="${prefix}shoulder_lift_joint" type="revolute">
    <parent link="${prefix}shoulder_link"/>
    <child link="${prefix}upper_arm_link"/>
    <origin xyz="0 0 0" rpy="1.570796327 0 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="150.0"
      lower="${joint_limits.joint_limits.shoulder_lift_joint.min_position}"
      upper="${joint_limits.joint_limits.shoulder_lift_joint.max_position}"
      velocity="${joint_limits.joint_limits.shoulder_lift_joint.max_velocity}" />
  </joint>

  <joint name="${prefix}elbow_joint" type="revolute">
    <parent link="${prefix}upper_arm_link"/>
    <child link="${prefix}forearm_link"/>
    <origin xyz="-0.24365 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="150.0"
      lower="${joint_limits.joint_limits.elbow_joint.min_position}"
      upper="${joint_limits.joint_limits.elbow_joint.max_position}"
      velocity="${joint_limits.joint_limits.elbow_joint.max_velocity}" />
  </joint>

  <joint name="${prefix}wrist_1_joint" type="revolute">
    <parent link="${prefix}forearm_link"/>
    <child link="${prefix}wrist_1_link"/>
    <origin xyz="-0.21325 0 0.11235" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="150.0"
      lower="${joint_limits.joint_limits.wrist_1_joint.min_position}"
      upper="${joint_limits.joint_limits.wrist_1_joint.max_position}"
      velocity="${joint_limits.joint_limits.wrist_1_joint.max_velocity}" />
  </joint>

  <joint name="${prefix}wrist_2_joint" type="revolute">
    <parent link="${prefix}wrist_1_link"/>
    <child link="${prefix}wrist_2_link"/>
    <origin xyz="0 -0.08535 -1.750557762378351e-11" rpy="1.570796327 0 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="150.0"
      lower="${joint_limits.joint_limits.wrist_2_joint.min_position}"
      upper="${joint_limits.joint_limits.wrist_2_joint.max_position}"
      velocity="${joint_limits.joint_limits.wrist_2_joint.max_velocity}" />
  </joint>

  <joint name="${prefix}wrist_3_joint" type="revolute">
    <parent link="${prefix}wrist_2_link"/>
    <child link="${prefix}wrist_3_link"/>
    <origin xyz="0 0.0819 -1.679797079540562e-11" rpy="1.570796326589793 3.141592653589793 3.141592653589793"/>
    <axis xyz="0 0 1"/>
    <limit effort="150.0"
      lower="${joint_limits.joint_limits.wrist_3_joint.min_position}"
      upper="${joint_limits.joint_limits.wrist_3_joint.max_position}"
      velocity="${joint_limits.joint_limits.wrist_3_joint.max_velocity}" />
  </joint>

</xacro:macro>
</robot>