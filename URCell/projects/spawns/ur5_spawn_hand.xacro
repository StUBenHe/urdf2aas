<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">
<xacro:macro name="ur5_spawn_hand" params="prefix xyz rpy">
  <!-- 加载 UR5 vistion mesh -->
<xacro:property name="mesh_root" value="/home/unvrx/URCell/types/ur_description/meshes" />
  <!-- 加载 UR5 关节限制参数 -->
  <xacro:property name="joint_limits"
                  value="${xacro.load_yaml('spawns/ur5_joint_limits.yaml')}" />

  <!-- ============================= -->
  <!--            LINKS             -->
  <!-- ============================= -->

  <link name="${prefix}base_link" />

  <link name="${prefix}base_link_inertia">
    <visual>
      <origin rpy="0 0 3.141592653589793" xyz="0 0 0" />
      <geometry>
        <mesh filename="file://${mesh_root}/ur5/visual/base.dae" />
      </geometry>
      <material name="LightGrey"><color rgba="0.7 0.7 0.7 1.0" /></material>
    </visual>
    <collision>
      <origin rpy="0 0 3.141592653589793" xyz="0 0 0" />
      <geometry>
        <mesh filename="file://${mesh_root}/ur5/collision/base.stl" />
      </geometry>
    </collision>
    <inertial>
      <mass value="4.0" />
      <origin rpy="0 0 0" xyz="0 0 0" />
      <inertia ixx="0.00443333156" ixy="0.0" ixz="0.0" iyy="0.00443333156" iyz="0.0" izz="0.0072" />
    </inertial>
  </link>

  <link name="${prefix}shoulder_link">
    <visual>
      <origin rpy="0 0 3.141592653589793" xyz="0 0 0" />
      <geometry>
        <mesh filename="file://${mesh_root}/ur5/visual/shoulder.dae" />
      </geometry>
      <material name="LightGrey"><color rgba="0.7 0.7 0.7 1.0" /></material>
    </visual>
    <collision>
      <origin rpy="0 0 3.141592653589793" xyz="0 0 0" />
      <geometry>
        <mesh filename="file://${mesh_root}/ur5/collision/shoulder.stl" />
      </geometry>
    </collision>
    <inertial>
      <mass value="3.7" />
      <origin rpy="0 0 0" xyz="0.0 -0.00193 -0.02561" />
      <inertia ixx="0.014972358333333331" ixy="0" ixz="0" iyy="0.014972358333333331" iyz="0" izz="0.01040625" />
    </inertial>
  </link>

  <link name="${prefix}upper_arm_link">
    <visual>
      <origin rpy="1.5707963267948966 0 -1.5707963267948966" xyz="0 0 0.13585" />
      <geometry>
        <mesh filename="file://${mesh_root}/ur5/visual/upperarm.dae" />
      </geometry>
      <material name="LightGrey"><color rgba="0.7 0.7 0.7 1.0" /></material>
    </visual>
    <collision>
      <origin rpy="1.5707963267948966 0 -1.5707963267948966" xyz="0 0 0.13585" />
      <geometry>
        <mesh filename="file://${mesh_root}/ur5/collision/upperarm.stl" />
      </geometry>
    </collision>
    <inertial>
      <mass value="8.393" />
      <origin rpy="0 1.570796326794897 0" xyz="-0.2125 0.0 0.11336" />
      <inertia ixx="0.13388583541666665" ixy="0" ixz="0" iyy="0.13388583541666665" iyz="0" izz="0.0151074" />
    </inertial>
  </link>

  <link name="${prefix}forearm_link">
    <visual>
      <origin rpy="1.5707963267948966 0 -1.5707963267948966" xyz="0 0 0.0165" />
      <geometry>
        <mesh filename="file://${mesh_root}/ur5/visual/forearm.dae" />
      </geometry>
      <material name="LightGrey"><color rgba="0.7 0.7 0.7 1.0" /></material>
    </visual>
    <collision>
      <origin rpy="1.5707963267948966 0 -1.5707963267948966" xyz="0 0 0.0165" />
      <geometry>
        <mesh filename="file://${mesh_root}/ur5/collision/forearm.stl" />
      </geometry>
    </collision>
    <inertial>
      <mass value="2.33" />
      <origin rpy="0 1.570796326794897 0" xyz="-0.24225 0.0 0.0265" />
      <inertia ixx="0.031216803515624995" ixy="0" ixz="0" iyy="0.031216803515624995" iyz="0" izz="0.004095" />
    </inertial>
  </link>

  <link name="${prefix}wrist_1_link">
    <visual>
      <origin rpy="1.5707963267948966 0 0" xyz="0 0 -0.093" />
      <geometry>
        <mesh filename="file://${mesh_root}/ur5/visual/wrist1.dae" />
      </geometry>
      <material name="LightGrey"><color rgba="0.7 0.7 0.7 1.0" /></material>
    </visual>
    <collision>
      <origin rpy="1.5707963267948966 0 0" xyz="0 0 -0.093" />
      <geometry>
        <mesh filename="file://${mesh_root}/ur5/collision/wrist1.stl" />
      </geometry>
    </collision>
    <inertial>
      <mass value="1.219" />
      <origin rpy="0 0 0" xyz="0.0 -0.01634 -0.0018" />
      <inertia ixx="0.002013889583333333" ixy="0" ixz="0" iyy="0.002013889583333333" iyz="0" izz="0.0021942" />
    </inertial>
  </link>

  <link name="${prefix}wrist_2_link">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 -0.095" />
      <geometry>
        <mesh filename="file://${mesh_root}/ur5/visual/wrist2.dae" />
      </geometry>
      <material name="LightGrey"><color rgba="0.7 0.7 0.7 1.0" /></material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 -0.095" />
      <geometry>
        <mesh filename="file://${mesh_root}/ur5/collision/wrist2.stl" />
      </geometry>
    </collision>
    <inertial>
      <mass value="1.219" />
      <origin rpy="0 0 0" xyz="0.0 0.01634 -0.0018" />
      <inertia ixx="0.0018310395833333333" ixy="0" ixz="0" iyy="0.0018310395833333333" iyz="0" izz="0.0021942" />
    </inertial>
  </link>

  <link name="${prefix}wrist_3_link">
    <visual>
      <origin rpy="1.5707963267948966 0 0" xyz="0 0 -0.0818" />
      <geometry>
        <mesh filename="file://${mesh_root}/ur5/visual/wrist3.dae" />
      </geometry>
      <material name="LightGrey"><color rgba="0.7 0.7 0.7 1.0" /></material>
    </visual>
    <collision>
      <origin rpy="1.5707963267948966 0 0" xyz="0 0 -0.0818" />
      <geometry>
        <mesh filename="file://${mesh_root}/ur5/collision/wrist3.stl" />
      </geometry>
    </collision>
    <inertial>
      <mass value="0.1879" />
      <origin rpy="0 0 0" xyz="0.0 0.0 -0.001159" />
      <inertia ixx="8.062475833333332e-05" ixy="0" ixz="0" iyy="8.062475833333332e-05" iyz="0" izz="0.0001321171875" />
    </inertial>
  </link>


  <!-- ============================= -->
  <!--            JOINTS            -->
  <!-- ============================= -->

  <joint name="${prefix}base_link-base_link_inertia" type="fixed">
    <parent link="${prefix}base_link" />
    <child link="${prefix}base_link_inertia" />
    <origin rpy="0 0 3.141592653589793" xyz="0 0 0" />
  </joint>

  <joint name="${prefix}shoulder_pan_joint" type="revolute">
    <parent link="${prefix}base_link_inertia" />
    <child link="${prefix}shoulder_link" />
    <origin rpy="0 0 0" xyz="0 0 0.089159" />
    <axis xyz="0 0 1" />
    <limit effort="150.0"
      lower="${joint_limits.joint_limits.shoulder_pan_joint.min_position}"
      upper="${joint_limits.joint_limits.shoulder_pan_joint.max_position}"
      velocity="${joint_limits.joint_limits.shoulder_pan_joint.max_velocity}" />
  </joint>

  <joint name="${prefix}shoulder_lift_joint" type="revolute">
    <parent link="${prefix}shoulder_link" />
    <child link="${prefix}upper_arm_link" />
    <origin rpy="1.570796327 0 0" xyz="0 0 0" />
    <axis xyz="0 0 1" />
    <limit effort="150.0"
      lower="${joint_limits.joint_limits.shoulder_lift_joint.min_position}"
      upper="${joint_limits.joint_limits.shoulder_lift_joint.max_position}"
      velocity="${joint_limits.joint_limits.shoulder_lift_joint.max_velocity}" />
  </joint>

  <joint name="${prefix}elbow_joint" type="revolute">
    <parent link="${prefix}upper_arm_link" />
    <child link="${prefix}forearm_link" />
    <origin rpy="0 0 0" xyz="-0.425 0 0" />
    <axis xyz="0 0 1" />
    <limit effort="150.0"
      lower="${joint_limits.joint_limits.elbow_joint.min_position}"
      upper="${joint_limits.joint_limits.elbow_joint.max_position}"
      velocity="${joint_limits.joint_limits.elbow_joint.max_velocity}" />
  </joint>

  <joint name="${prefix}wrist_1_joint" type="revolute">
    <parent link="${prefix}forearm_link" />
    <child link="${prefix}wrist_1_link" />
    <origin rpy="0 0 0" xyz="-0.39225 0 0.10915" />
    <axis xyz="0 0 1" />
    <limit effort="28.0"
      lower="${joint_limits.joint_limits.wrist_1_joint.min_position}"
      upper="${joint_limits.joint_limits.wrist_1_joint.max_position}"
      velocity="${joint_limits.joint_limits.wrist_1_joint.max_velocity}" />
  </joint>

  <joint name="${prefix}wrist_2_joint" type="revolute">
    <parent link="${prefix}wrist_1_link" />
    <child link="${prefix}wrist_2_link" />
    <origin rpy="1.570796327 0 0" xyz="0 -0.09465 0" />
    <axis xyz="0 0 1" />
    <limit effort="28.0"
      lower="${joint_limits.joint_limits.wrist_2_joint.min_position}"
      upper="${joint_limits.joint_limits.wrist_2_joint.max_position}"
      velocity="${joint_limits.joint_limits.wrist_2_joint.max_velocity}" />
  </joint>

  <joint name="${prefix}wrist_3_joint" type="revolute">
    <parent link="${prefix}wrist_2_link" />
    <child link="${prefix}wrist_3_link" />
    <origin rpy="1.570796326589793 3.141592653589793 3.141592653589793" xyz="0 0.0823 0" />
    <axis xyz="0 0 1" />
    <limit effort="28.0"
      lower="${joint_limits.joint_limits.wrist_3_joint.min_position}"
      upper="${joint_limits.joint_limits.wrist_3_joint.max_position}"
      velocity="${joint_limits.joint_limits.wrist_3_joint.max_velocity}" />
  </joint>

</xacro:macro>

</robot>
